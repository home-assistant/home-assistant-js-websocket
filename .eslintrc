{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "rules": {
    "no-plusplus": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "func-names": 0,
    "no-multi-assign": 0,
    "prefer-destructuring": 0,
    "import/extensions": 0,
    "no-restricted-globals": 0,
    "import/prefer-default-export": 0
  }
}
